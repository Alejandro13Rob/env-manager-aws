{
  "name": "@discovery-latam/env-manager-aws",
  "version": "1.0.0",
  "description": "Library that allows to configure environment variables in files and to be able to quickly switch between environments using AWS Parameter Store",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "start": "tsc --build --watch",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "test": "jest",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "patch:publish": "npm version patch && npm publish",
    "link": "npm link"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Alejandro13Rob/env-manager-aws.git"
  },
  "keywords": [
    "AWS",
    "parameter store",
    "AWS Parameter Store"
  ],
  "author": "R-M",
  "license": "MIT",
  "devDependencies": {
    "@babel/parser": "^7.14.7",
    "@babel/types": "^7.14.5",
    "@types/jest": "^26.0.23",
    "jest": "^27.0.6",
    "prettier": "^2.3.2",
    "ts-jest": "^27.0.3",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.3.4",
    "find-up": "5.0.0"
  },
  "files": [
    "dist/**/*"
  ],
  "bin": {
    "env-manager": "dist/cli.js"
  },
  "bugs": {
    "url": "https://github.com/Alejandro13Rob/env-manager-aws/issues"
  },
  "homepage": "https://github.com/Alejandro13Rob/env-manager-aws#readme"
}